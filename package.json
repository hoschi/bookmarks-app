{
  "title": "Bookmarks App",
  "description": "Apollo with GraphQL fake server",
  "dependencies": {
    "@material-ui/core": "3.1.2",
    "@material-ui/icons": "3.0.1",
    "apollo-boost": "0.1.16",
    "classnames": "2.2.6",
    "graphql": "14.0.2",
    "prop-types": "15.6.2",
    "ramda": "0.25.0",
    "react": "16.5.2",
    "react-apollo": "2.2.4",
    "react-dom": "16.5.2"
  },
  "name": "bookmarks-app",
  "version": "0.0.0",
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-jest": "21.24.1",
    "eslint-plugin-react": "7.11.1",
    "json-graphql-server": "2.1.2",
    "prettier": "1.14.3",
    "react-scripts": "2.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:server": "clear; json-graphql-server config/db.js --p 3009",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{js,jsx,json,scss,css,html}'",
    "lint:js:check": "eslint .",
    "lint:js:fix": "eslint --fix .",
    "format-and-fix": "npm run lint:js:fix && npm run format"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
